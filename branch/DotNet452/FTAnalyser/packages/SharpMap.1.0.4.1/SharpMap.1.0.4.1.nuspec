  <metadata>
    <id>SharpMap</id>
    <version>1.0.4.1</version>
    <authors>SharpMap-Team</authors>
    <owners>SharpMap-Team</owners>
    <licenseUrl>http://sharpmap.codeplex.com/license</licenseUrl>
    <projectUrl>http://sharpmap.codeplex.com/</projectUrl>
